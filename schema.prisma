model Bill {
  id        String    @id @default(cuid())
  user      String?
  createdAt DateTime? @default(now())
  updatedAt DateTime? @default(now())
  User      User?     @relation(fields: [user], references: [id])
  Extra     Extra[]

  @@index([user])
}

model Extra {
  id          String    @id @default(cuid())
  bill        String?
  name        String    @default("")
  description String    @default("")
  price       Float?
  amount      Int?
  stripeId    String    @unique @default("")
  createdAt   DateTime? @default(now())
  updatedAt   DateTime? @default(now())
  Bill        Bill?     @relation(fields: [bill], references: [id])

  @@index([bill])
}

model VIPLike {
  id             String  @id @default(cuid())
  user           String?
  track          String?
  userId_trackId String  @unique @default("")
  Track          Track?  @relation(fields: [track], references: [id])
  User           User?   @relation(fields: [user], references: [id])

  @@index([track])
  @@index([user])
}

model Listened {
  id             String    @id @default(cuid())
  user           String?
  track          String?
  userId_trackId String    @unique @default("")
  createdAt      DateTime? @default(now())
