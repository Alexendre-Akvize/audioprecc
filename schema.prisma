// Prisma schema for ID By Rivoli
// This schema should match your existing database structure
// Run `prisma db pull` to introspect and update from existing database

generator client {
  provider             = "prisma-client-py"
  interface            = "sync"
  recursive_type_depth = 5
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id                      String                   @id @default(uuid())
  name                    String                   @default("")
  email                   String                   @unique @default("")
  emailVerified           DateTime?
  image_id                String?
  image_filesize          Int?
  image_width             Int?
  image_height            Int?
  image_extension         String?
  password                String?
  createdAt               DateTime?                @default(now())
  updatedAt               DateTime?                @updatedAt
  type                    Json                     @default("[]")
  isFree                  Boolean                  @default(false)
  downloadCount           Int?                     @default(0)
  stripe_customer_id      String                   @default("")
  billing_address         String                   @default("")
  isVIP                   Boolean                  @default(false)
  accountActivated        Boolean                  @default(false)
  artistName              String                   @default("")
  deletedAt               DateTime?
  age                     Int?
  instagramUrl            String                   @default("")
  phone                   String                   @default("")
  soundcloudURL           String                   @default("")
  spotifyURL              String                   @default("")
  tiktokUrl               String                   @default("")
  twitterUrl              String                   @default("")
  DJName                  String                   @default("")
  favoriteStyles          Json                     @default("[]")
  favoritesFormat         String?
  avatar_extension        String?
  avatar_filesize         Int?
  avatar_height           Int?
  avatar_id               String?
  avatar_width            Int?
  handle                  String                   @default("")
  forumVerified           Boolean                  @default(false)
  profileColor            String                   @default("")
  isProducer              Boolean                  @default(false)
  contrat1Accepted        Boolean                  @default(false)
  contrat2Accepted        Boolean                  @default(false)
  isAdmin                 Boolean                  @default(false)
  city                    String                   @default("")
  longBio                 String                   @default("")
  realName                String                   @default("")
  shortBio                String                   @default("")
  websiteUrl              String                   @default("")
  youtubeUrl              String                   @default("")
  AccapellaLike           AccapellaLike[]
  Account                 Account[]
  Authenticator           Authenticator[]
  Bill                    Bill[]
  Blog                    Blog[]
  CommunicationPreference CommunicationPreference?
  Credit                  Credit?
  Download                Download[]
  ForumComment            ForumComment[]
  ForumPost               ForumPost[]
  ForumVideo              ForumVideo[]
  InstrumentalLike        InstrumentalLike[]
  InteractionForumPost    InteractionForumPost[]
  IntroLike               IntroLike[]
  Like                    Like[]
  Listened                Listened[]
  Mix                     Mix[]
  Playlist                Playlist[]
  PressInformation        PressInformation?
  PressKit                PressKit[]
  Session                 Session[]
  ShortLike               ShortLike[]
  Subscription            Subscription?
  SuperShortLike          SuperShortLike[]
  Track                   Track[]
  VIPLike                 VIPLike[]
  Artist                  Artist[]                 @relation("Artist_users")
  ProducerRequest         ProducerRequest[]        @relation("ProducerRequest_user")
  Search                  Search[]                 @relation("Search_user")
  Tag                     Tag[]                    @relation("Tag_users")
}

model Account {
  id                         String    @id @default(cuid())
  user                       String?
  type                       String    @default("")
  provider                   String    @default("")
  providerAccountId          String    @default("")
  refresh_token              String    @default("")
  access_token               String    @default("")
  expires_at                 Int?
  token_type                 String    @default("")
  scope                      String    @default("")
  id_token                   String    @default("")
  session_state              String    @default("")
  createdAt                  DateTime? @default(now())
  updatedAt                  DateTime? @default(now())
  provider_providerAccountId String    @unique @default("")
  User                       User?     @relation(fields: [user], references: [id])

  @@index([user])
}

model Session {
  id           String    @id @default(uuid())
  sessionToken String    @unique @default("")
  user         String?
  expires      DateTime?
  createdAt    DateTime? @default(now())
  updatedAt    DateTime? @default(now())
  User         User?     @relation(fields: [user], references: [id])

  @@index([user])
}

model VerificationToken {
  id                                      String              @id @default(uuid())
  identifier                              String              @default("")
  token                                   String              @unique @default("")
  expires                                 DateTime?
  identifier_tokenId                      String?             @map("identifier_token")
  maxUses                                 Int?                @default(10)
  usedCount                               Int?                @default(0)
  used                                    Int?                @default(0)
  identifier_token                        VerificationToken?  @relation("VerificationToken_identifier_token", fields: [identifier_tokenId], references: [id])
  from_VerificationToken_identifier_token VerificationToken[] @relation("VerificationToken_identifier_token")

  @@index([identifier_tokenId])
}

model Authenticator {
  id                   String  @id @default(cuid())
  credentialID         String  @unique @default("")
  user                 String?
  providerAccountId    String  @default("")
  credentialPublicKey  String  @default("")
  counter              Int?
  credentialDeviceType String  @default("")
  credentialBackedUp   Boolean @default(false)
  transports           String  @default("")
  User                 User?   @relation(fields: [user], references: [id])

  @@index([user])
}

model Subscription {
  id                   String    @id @default(cuid())
  user                 String?   @unique
  stripeId             String    @unique @default("")
  plan                 String?
  status               String?   @default("active")
  price_id             String    @default("")
  quantity             Int?
  cancel_at_period_end DateTime?
  cancel_at            DateTime?
  canceled_at          DateTime?
  current_period_start DateTime?
  current_period_end   DateTime?
  created              DateTime?
  ended_at             DateTime?
  trial_start          DateTime?
  trial_end            DateTime?
  currentPeriodEnd     DateTime?
  createdAt            DateTime? @default(now())
  updatedAt            DateTime? @default(now())
  Plan                 Plan?     @relation(fields: [plan], references: [id])
  User                 User?     @relation(fields: [user], references: [id])

  @@index([plan])
}

model Credit {
  id        String    @id @default(cuid())
  user      String?   @unique
  amount    Int?      @default(0)
  createdAt DateTime? @default(now())
  updatedAt DateTime? @default(now())
  User      User?     @relation(fields: [user], references: [id])
}

