model Tag {
  id    String  @id @default(cuid())
  name  String  @default("")
  Track Track[] @relation("Tag_tracks")
  User  User[]  @relation("Tag_users")
}

model Mix {
  id                   String    @id @default(cuid())
  user                 String?
  title                String    @default("")
  tracks               String?
  bpmRange             String    @default("")
  mixType              String    @default("")
  description          String    @default("")
  coverImage_id        String?
  coverImage_filesize  Int?
  coverImage_width     Int?
  coverImage_height    Int?
  coverImage_extension String?
  tracklist            Json?
  genre                String    @default("")
  createdAt            DateTime? @default(now())
  updatedAt            DateTime? @default(now())
  mixTrackList         String    @default("")
  Track                Track?    @relation(fields: [tracks], references: [id])
  User                 User?     @relation(fields: [user], references: [id])
  Artist               Artist[]  @relation("Artist_mixes")

  @@index([user])
  @@index([tracks])
}

model Blog {
  id              String    @id @default(cuid())
  user            String?
  title           String    @default("")
  excerpt         String    @default("")
  image_id        String?
  image_filesize  Int?
  image_width     Int?
  image_height    Int?
  image_extension String?
  publishedAt     DateTime? @default(now())
  createdAt       DateTime? @default(now())
  updatedAt       DateTime? @default(now())
  content         Json      @default("[{\"type\": \"paragraph\", \"children\": [{\"text\": \"\"}]}]")
  User            User?     @relation(fields: [user], references: [id])

  @@index([user])
}

model Download {
  id        String    @id @default(cuid())
  user      String?
  track     String?
  createdAt DateTime? @default(now())
  updatedAt DateTime? @default(now())
  Track     Track?    @relation(fields: [track], references: [id])
  User      User?     @relation(fields: [user], references: [id])

  @@index([track])
  @@index([user])
}

model Playlist {
  id                     String    @id @default(cuid())
  title                  String    @default("")
  energyLevel            String    @default("")
  createdAt              DateTime? @default(now())
  updatedAt              DateTime? @default(now())
  cover_id               String?
  cover_filesize         Int?
  cover_width            Int?
  cover_height           Int?
  cover_extension        String?
  slug                   String    @unique @default("")
  description            String    @default("")
  isHighlighted          Boolean   @default(false)
  duration               Float?    @default(0)
  isBloc                 Boolean   @default(false)
  jsonData               Json?     @map("json")
  mixFile_filename       String?
  mixFile_filesize       Int?
  user                   String?
  color                  String    @default("")
  isPublic               Boolean   @default(false)
  category               String?
  styles                 Json      @default("[]")
  previewCover_extension String?
  previewCover_filesize  Int?
  previewCover_height    Int?
  previewCover_id        String?
  previewCover_width     Int?
  User                   User?     @relation(fields: [user], references: [id])
  Artist                 Artist[]  @relation("Artist_playlists")
  Track                  Track[]   @relation("Playlist_tracks")

  @@index([user])
}

model CommunicationPreference {
  id       String  @id @default(cuid())
  user     String? @unique
  email    Boolean @default(true)
  whatsapp Boolean @default(false)
  User     User?   @relation(fields: [user], references: [id])
}

model Like {
  id             String    @id @default(cuid())
  user           String?
  track          String?
  userId_trackId String    @unique @default("")
  createdAt      DateTime? @default(now())
  updatedAt      DateTime? @default(now())
  Track          Track?    @relation(fields: [track], references: [id])
  User           User?     @relation(fields: [user], references: [id])

  @@index([track])
  @@index([user])
}

model Plan {
  id                  String         @id @default(cuid())
  name                String         @default("")
  description         String         @default("")
  monthlyPrice        Float?
  yearlyPrice         Float?
  stripeMonthlyUrl    String         @default("")
  stripeYearlyUrl     String         @default("")
  stripeMonlthlyId    String         @unique @default("")
  stripeyearlyId      String         @unique @default("")
  createdAt           DateTime?      @default(now())
  updatedAt           DateTime?      @default(now())
  montlyAmount        Int?
  yearlyAmount        Int?
  basePrice           Float?
  promoCode           String         @default("")
  stripeMonlthlyIdEUR String         @unique @default("")
  stripeyearlyIdEUR   String         @unique @default("")
  Subscription        Subscription[]
}

